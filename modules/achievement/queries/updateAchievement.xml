<query id="updateAchievement" action="update">
    <tables>
        <table name="achievement" />
    </tables>
     <conditions>
        <condition operation="equal" column="achievement_srl" var="achievement_srl" filter="number" notnull="notnull" />
    </conditions>
	<columns>
        <column name="achievement_srl" var="achievement_srl"/>
        <column name="module_srl"  var="module_srl" />
        <column name="achievement_class_srl"  var="achievement_class_srl" />
        <column name="achievement_title"  var="achievement_title" />
        <column name="achievement_owner"  var="achievement_owner" />
        <column name="achievement_woncheongsa"  var="achievement_woncheongsa" />
        <column name="achievement_position"  var="achievement_position" />
        <column name="achievement_start_date"  var="achievement_start_date" />
        <column name="achievement_end_date"  var="achievement_end_date" />
    </columns>
</query>
