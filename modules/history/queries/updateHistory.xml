<query id="updateHistory" action="update">
    <tables>
        <table name="historys" />
    </tables>
     <conditions>
        <condition operation="equal" column="history_srl" var="history_srl" filter="number" notnull="notnull" />
    </conditions>
	<columns>
        <column name="module_srl" var="module_srl" notnull="notnull" />
        <column name="history_date" var="history_date" />
        <column name="history_decs" var="history_decs" />
    </columns>
</query>
